<p id="notice"><%= notice %></p>
<h1 class="display-1">Image Details</h1>
<hr>
<table class="table table-bordered table-responsive">
	<tbody>
		<tr>
			<th scope="row">Image</th>
			<td><%= image_tag @cms_slideshow_image.image %></td>
		</tr>
		<tr>
			<th scope="row">Destination</th>
			<td><%= @cms_slideshow_image.destination %></td>
		</tr>
	</tbody>
</table>
<button type="button" class="btn btn-secondary btn-lg"><%= link_to 'Edit', edit_cms_slideshow_image_path(@cms_slideshow_image) %></button>
<button type="button" class="btn btn-secondary btn-lg"><%= link_to 'Back', cms_slideshow_images_path %></button>